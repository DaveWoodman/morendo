      <version>1.0</version>
      <models>
        <org.jamocha.service.ObjectModel>
          <classList>
            <string>woolfel.examples.model.Account</string>
            <string>woolfel.examples.model.Account2</string>
            <string>woolfel.examples.model.Hobby</string>
